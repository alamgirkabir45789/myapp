<script type="text/javascript">
    $(document).ready(function () {
        console.log($('#txt-footer-subscribe-email'))
       
    });

    function SubscribePost() {
        var email = $('#txt-footer-subscribe-email').val();
        if (email != '') {
            Common.Ajax('GET', '/global/api/home/subscribe?email=' + email, [], 'json', ajaxSubscribe, false);
        }
    }

    function ajaxSubscribe(response) {
        if(response !=null){
            Swal.fire("Thank you for your Query!", "We will get back to you!", "success");
        }else{

        Swal.fire("Already subscribe!", "Thanks!", "warning");
        }
        
        $('#txt-footer-subscribe-email').val("");
      
    }
</script>